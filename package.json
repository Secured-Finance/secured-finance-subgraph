{
  "name": "secured-finance-subgraph",
  "version": "0.2.0-beta.34",
  "private": "true",
  "scripts": {
    "generate:dev": "ts-node ./scripts/generate.ts development goerli",
    "generate:stg": "ts-node ./scripts/generate.ts staging goerli",
    "codegen": "graph codegen ./subgraph.yaml",
    "precodegen": "ts-node ./scripts/copy-abis.ts",
    "build": "graph build ./subgraph.yaml",
    "prebuild": "npm run codegen",
    "test": "graph test",
    "coverage": "graph test -- -c",
    "auth": "graph auth --studio",
    "deploy:dev": "cat ./subgraph.development.yaml && graph deploy ./subgraph.development.yaml --studio sf-protocol-dev --output-dir ./build/development --versionLabel",
    "deploy:stg": "cat ./subgraph.staging.yaml && graph deploy ./subgraph.staging.yaml --studio sf-protocol-stg --output-dir ./build/staging --versionLabel",
    "prepublish": "npm run codegen",
    "update-subgraphrc": "ts-node ./scripts/update-subgraphrc.ts",
    "format": "prettier --config ./.prettierrc.json --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}'",
    "format:fix": "prettier --config ./.prettierrc.json --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}'"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.37.6",
    "@graphprotocol/graph-ts": "^0.29.1",
    "@secured-finance/smart-contracts": "0.0.5-beta.26",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.14.2",
    "js-yaml": "^4.1.0",
    "matchstick-as": "0.5.0",
    "ts-node": "^10.9.1",
    "prettier": "^2.8.3"
  }
}
